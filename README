  /usr/share/doc/opencdk-0.5.13/README
  /usr/share/doc/opencdk-0.5.13/THANKS
  /usr/share/doc/opencdk-0.5.13/TODO

(libopencdk8)
  /usr/bin/cygopencdk-8.dll

(libopencdk-devel)
  /usr/bin/opencdk-config
  /usr/include/opencdk.h
  /usr/lib/libopencdk.a
